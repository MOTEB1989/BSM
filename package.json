{
  "name": "bsu",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "validate": "node scripts/validate.js",
    "test": "npm run validate && mocha tests/**/*.test.js --timeout 5000",
    "test:unit": "mocha tests/**/*.test.js --timeout 5000",
    "test:coverage": "c8 --reporter=text --reporter=html --reporter=lcov mocha tests/**/*.test.js --timeout 5000",
    "test:observatory": "mocha tests/services/observatory*.test.js tests/services/alertService.test.js tests/middleware/*.test.js tests/routes/observatory.test.js --timeout 5000",
    "pr-check": "node scripts/pr-review-checklist.js --local",
    "pr-check:verbose": "node scripts/pr-review-checklist.js --local --verbose"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "chart.js": "^4.5.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.77.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.18.0",
    "pino": "^9.0.0",
    "sentiment": "^5.0.2",
    "socket.io": "^4.8.3",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "chai": "^6.2.2",
    "mocha": "^11.7.5",
    "nodemon": "^3.1.0",
    "pino-pretty": "^11.0.0",
