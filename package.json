{
  "name": "bsu",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "src/server.js",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "lint": "npm run validate:registry && npm run validate",
    "validate": "node scripts/validate.js",
    "validate:registry": "node scripts/validate-registry.js",
    "validate:agent-sync": "node scripts/validate-agent-sync.js",
    "validate:render": "node scripts/validate-render.js",
    "test": "node scripts/validate.js",
    "check:tools": "node scripts/check-required-tools.js",
    "check:duplicates": "node scripts/prevent-duplicate-agents.js",
    "merge:agents": "node scripts/merge-agents.js",
    "precommit": "npm run check:duplicates",
    "install:hooks": "bash .githooks/install.sh",
    "ci:check": "npm run check:tools && npm run lint && npm run test:unit",
    "health": "node scripts/health-check.js",
    "health:detailed": "node scripts/health-check.js --detailed",
    "pr-check": "node scripts/pr-review-checklist.js --local",
    "pr-check:verbose": "node scripts/pr-review-checklist.js --local --verbose",
    "test:unit": "node --test tests/*.test.js",
    "prepare": "node scripts/install-git-hooks.js",
    "mcp:install": "cd mcp-servers && npm install",
    "mcp:start": "node mcp-servers/bsu-agent-server.js",
    "postinstall": "npm run mcp:install",
    "agents:integrity": "node scripts/run-integrity-check.js",
    "agents:audit": "node scripts/audit-runner.js",
    "agents:orchestrate": "bash scripts/run_agents.sh"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "node-fetch": "^3.3.2",
    "pino": "^9.0.0",
    "yaml": "^2.4.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "pino-pretty": "^11.0.0"
  },
  "overrides": {
    "minimatch": "^10.2.1"
  }
}
